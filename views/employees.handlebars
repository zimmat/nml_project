<h2>Employees</h2>
<span class="counter pull-right"></span>
<table class="table table-hover table-bordered results">
  <thead>
    <tr>
      <th class="col-md-1 col-xs-1">Name</th>
      <th class="col-md-3 col-xs-3">Surname</th>
      <th class="col-md-3 col-xs-3">Email</th>
    </tr>
  </thead>
  <tbody>
      {{#employees}}
    <tr>
      <td>{{name}}</td>
      <td>{{surname}}</td>
      <td>{{email}}</td>
    </tr>
      {{/employees}}>
  </tbody>
</table>
